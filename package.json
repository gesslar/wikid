{
  "name": "@gesslar/wikid",
  "version": "2.1.0",
  "description": "If Salt'n'Pepa wrote a MediaWiki auth package in Node.js",
  "type": "module",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js"
    }
  },
  "files": [
    "src",
    "README.md",
    "UNLICENSE.txt"
  ],
  "scripts": {
    "dev": "node src/index.js",
    "test": "node tests/unit/Wikid.test.js",
    "types:build": "tsc -p tsconfig.types.json",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "submit": "npm publish --access public",
    "update": "npx npm-check-updates -u && npm install",
    "pr": "gt submit --publish --restack --ai",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "keywords": [
    "mediawiki",
    "wiki",
    "media",
    "utilities",
    "esm",
    "push",
    "it",
    "real",
    "good"
  ],
  "author": "gesslar",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gesslar/wikid.git"
  },
  "homepage": "https://github.com/gesslar/wikid#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/node": "^24.10.0",
    "eslint": "^9.39.0",
    "eslint-plugin-jsdoc": "^61.1.12",
    "globals": "^16.5.0"
  },
  "dependencies": {
    "@gesslar/toolkit": "^0.7.0",
    "node-fetch": "^3.3.2"
  }
}
